    <center><h1>Students Database</h1></center>


    <table id="students">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Roll Number</th>
        <th>Branch</th>
        <th>CGPA</th>
        <th>Percentage</th>
        <th>Language 1</th>
        <th>Language 2</th>
        <th>Technology 1</th>
        <th>Technology 2</th>
        <th>Resume Link</th>
        <th>Drive Link</th>
      </tr>

      <% students.forEach(student => { %>

        <tr>
          <td><%= student.name %></td>
          <td><%= student.email %></td>
          <td><%= student.rno %></td>
          <td><%= student.branch %></td>
          <td><%= student.gpa %></td>
          <td><%= student.perc %></td>
          <td><%= student.lang1 %></td>
          <td><%= student.lang2 %></td>
          <td><%= student.tech1 %></td>
          <td><%= student.tech2 %></td>
          <td><a href="<%= student.resumelink %>" target='_blank'>Open</a></td>
          <td><a href="<%= student.drivelink %>" target='_blank'>Open</a></td>       
        </tr>
    <% }) %>
      
    </table>
     
  
   
    
    
    <script src="/styles/landingpage.js"></script> 
    
